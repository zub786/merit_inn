<p><h3><b style="text-align:right;">Total Monthly Revenue : </b> @ViewBag.TotalMonthlyExpense </h3></p>

<table class=" table table-hover">
    <thead>
        <tr>
            <th>Student Name</th>
            <th>Submission Date</th>

            <th>
                Amount
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var Temp in ViewBag.TotalMonthlyExpenseDetails)
        {


            <tr>

                <td>@Temp.Student.StudentName</td>
                <td>@Temp.Date</td>

                <td>@Temp.FeeAmount</td>

            </tr>

        }

    </tbody>


</table>


