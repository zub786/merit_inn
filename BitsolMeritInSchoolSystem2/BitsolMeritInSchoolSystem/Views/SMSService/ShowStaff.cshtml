




<hr />
<table id="AllStaffTable" class="table table-hover" style="width:100%;background-color:grey;margin-top:10px;">

    <thead>
        <tr>
            <th style="text-align: center;">
                Select
            </th>
            <th style="text-align: center;">
                Name
            </th>
            <th style="text-align: center;">
                CNIC #
            </th>
            <th style="text-align: center;">
                Email
            </th>
            <th style="text-align: center;">
                Contact#
            </th>
            <th style="text-align: center;">
                Designation
            </th>
            <th style="text-align: center;">
                Qualification
            </th>

        </tr>
    </thead>
    <tbody>

        @foreach (var m in ViewBag.Members)
        {
            <tr>
                <td style="text-align: center;">
                    <input type="checkbox" class="form-control" value="@m.StaffMemberId" id="SelectiveStaff"/>

                </td>
                <td style="text-align: center;">
                    @m.StaffMemberName

                </td>
                <td style="text-align: center;">
                    @m.StaffMemberCNIC

                </td>
                <td style="text-align: center;">
                    @m.StaffMemberEmail

                </td>
                <td style="text-align: center;">
                    @m.StaffMemberContactNo

                </td>
                <td style="text-align: center;">
                    @m.Designation

                </td>
                <td style="text-align: center;">
                    @m.Qualification

                </td>


            </tr>
        }

    </tbody>


</table>

