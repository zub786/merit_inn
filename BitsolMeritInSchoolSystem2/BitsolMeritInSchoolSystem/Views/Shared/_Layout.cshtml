<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   
    <title>@ViewBag.Title - Approach 2 Solution Pvt Ltd.</title>
    <link href="~/Content/themes/base/all.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.1.0.min.js"></script>
    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

  
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>

    <script src="~/Scripts/jquery-ui-1.12.0.min.js"></script>

   
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
   
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/dataTables.jqueryui.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />

    <link href="~/Content/themes/styles.css" rel="stylesheet" />
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>



</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Merit Inn", "HomePage", "Home", null, new { @class = "navbar-brand " })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="color: #FFFFFF;">@Html.ActionLink("Administrator", "Admin", "Administrator")</li>
                    <li style="color: #FFFFFF;">@Html.ActionLink("Attendence", "Index", "Attendence")</li>
                </ul>
            </div>
        </div>
    </div>

































  


  
    <div class="row">
        <div class="col-md-12">
            @RenderBody()

        </div>

    </div>

    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year -  Developed By <a href="http://approach-2-solution.com"> Approach 2 Solutions Pvt Ltd. </a></p>
    </footer>
       
    
  

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)
</body>
</html>
